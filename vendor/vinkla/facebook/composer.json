{
    "name": "vinkla/facebook",
    "description": "A Facebook bridge for Laravel",
    "license": "MIT",
    "keywords": ["facebook", "manager", "laravel"],
    "authors": [
        {
            "name": "Vincent Klaiber",
            "email": "hello@vinkla.com",
            "homepage": "https://vinkla.com"
        }
    ],
    "require": {
        "php": "^5.6.4 || ^7.0",
        "illuminate/contracts": "5.2.* || 5.3.*",
        "illuminate/support": "5.2.* || 5.3.*",
        "graham-campbell/manager": "^2.4",
        "facebook/graph-sdk": "^5.4"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.2",
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "Vinkla\\Facebook\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vinkla\\Tests\\Facebook\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.5-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
